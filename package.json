    "lodash": "^4.17.20",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "engines": {
    "node": ">=10"
  },
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/npm",
      "@semantic-release/git"
    ]
  }
}
